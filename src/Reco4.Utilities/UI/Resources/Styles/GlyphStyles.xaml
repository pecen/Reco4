  <Style x:Key="GlyphTextStyle">
    <Setter Property="TextElement.FontFamily" Value="{StaticResource glyph_Reco4FontFamily}" />
    <Setter Property="TextElement.FontSize" Value="{StaticResource glyph_FontSize}" />
    <Setter Property="TextOptions.TextRenderingMode" Value="Aliased" />
    <Setter Property="TextOptions.TextFormattingMode" Value="Ideal" />
    <Setter Property="TextBlock.VerticalAlignment" Value="Center" />
    <Setter Property="TextBlock.HorizontalAlignment" Value="Center" />
    <Setter Property="TextBlock.SnapsToDevicePixels" Value="True" />
  </Style>

  <Style x:Key="GlyphTextErrorStyle" BasedOn="{StaticResource GlyphTextStyle}">
    <Setter Property="TextElement.Foreground" Value="Red" />
  </Style>

  <Style x:Key="GlyphTextWarningStyle" BasedOn="{StaticResource GlyphTextStyle}">
    <Setter Property="TextElement.Foreground" Value="Orange" />
  </Style>

  <Style x:Key="GlyphTextInformationStyle" BasedOn="{StaticResource GlyphTextStyle}">
    <Setter Property="TextElement.Foreground" Value="Blue" />
  </Style>

  <Style x:Key="GlyphTextOkStyle" BasedOn="{StaticResource GlyphTextStyle}">
    <Setter Property="TextElement.Foreground" Value="Green" />
  </Style>
